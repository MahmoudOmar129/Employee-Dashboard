@model DepartmentVM




@{
    ViewData["Title"] = "Details";
}


<div class="content-wrapper">

    <div class="content-header">
        <div class="container">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <a class="btn btn-success font-weight-bold" asp-controller="Department" asp-action="Index">Return</a>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Departments</a></li>
                        <li class="breadcrumb-item active">Department Details</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>




    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <!-- general form elements -->
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title">@Model.Name Department</h3>
                    </div>
                    <!-- /.card-header -->
                    <!-- form start -->
                    <form asp-controller="Department" asp-action="Create">
                        <div class="card-body">
                            <div class="form-group">
                                <label asp-for="Name">Name</label>
                                <input type="text" class="form-control" asp-for="Name" placeholder="Enter Name" autofocus autocomplete="off" disabled>
                            </div>
                            <div class="form-group">
                                <label asp-for="Code">Code</label>
                                <input type="text" class="form-control" asp-for="Code" placeholder="Enter Code" autocomplete="off" disabled>
                            </div>
                        </div>
                        <!-- /.card-body -->
                    </form>
                </div>
                <!-- /.card -->
                <center>
                    <a class="btn btn-danger  font-weight-bold" asp-controller="Department" asp-action="Delete" asp-route-id="@Model.Id">Delete</a>
                    <a class="btn btn-warning font-weight-bold" asp-controller="Department" asp-action="Update" asp-route-id="@Model.Id">Update</a>
                </center>
            </div>
        </div>

    </div>



</div>


